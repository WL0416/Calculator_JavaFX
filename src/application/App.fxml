<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controller">
   <children>
      <TextField fx:id="firstnum" layoutX="66.0" layoutY="42.0" prefHeight="23.0" prefWidth="81.0" />
      <TextField fx:id="secondnum" layoutX="158.0" layoutY="42.0" prefHeight="23.0" prefWidth="81.0" />
      <Button fx:id="addbutton" layoutX="75.0" layoutY="81.0" mnemonicParsing="false" onAction="#add" prefWidth="64.0" text="Add" textAlignment="CENTER" />
      <Button fx:id="multiplybutton" layoutX="75.0" layoutY="116.0" mnemonicParsing="false" onAction="#multiply" text="Multiply" textAlignment="CENTER" />
      <Button fx:id="minusbutton" layoutX="167.0" layoutY="81.0" mnemonicParsing="false" onAction="#minus" prefWidth="64.0" text="Minus" textAlignment="CENTER" />
      <TextField fx:id="result" layoutX="126.0" layoutY="155.0" prefHeight="23.0" prefWidth="102.0" />
      <Button fx:id="dividebutton" layoutX="167.0" layoutY="116.0" mnemonicParsing="false" onAction="#divide" prefWidth="64.0" text="Divide" textAlignment="CENTER" />
      <Label fx:id="label1" layoutX="82.0" layoutY="159.0" text="Result:" />
      <Label fx:id="label2" layoutX="89.0" layoutY="15.0" text="Type in numbers here." />
   </children>
</AnchorPane>
